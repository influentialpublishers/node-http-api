{"changed":true,"filter":false,"title":"api.coffee","tooltip":"/src/api.coffee","value":"request     = require 'request'\nQueue       = require 'pipeline-queue'\nisArray     = require('util').isArray\n\nparsePath   = (path, url) ->\n    path    = [path] if !isArray path\n    path.unshift(url)\n    return path.join '/'\n    \nparseOptions    = (options) ->\n    parsed  =\n        method: options?.method ? 'GET' || options.method.toUpperCase()\n        url: parsePath options.path, options.url\n        headers: options.headers\n        \n    if typeof options.params isnt 'function'\n        if parsed.method is 'GET'\n            parsed.qs   = options.params\n        else\n            parsed.body = options.params\n            parsed.json = true\n        \n    return parsed\n    \ncall    = (options, done) ->\n    request options, (err, httpResponse, body) ->\n        if not err and httpResponse.statusCode is 200\n            try\n                body    = JSON.parse body\n            catch parse_error\n                err     = parse_error\n        else if httpResponse.statusCode isnt 200\n            err =\n                message: body || httpResponse.body || httpResponse.text\n                code: httpResponse.statusCode\n                \n        return done err, httpResponse, body\n        \nmodule.exports  = (config) ->\n    \n    methodFactory   = (method) ->\n        options = \n            method: method\n            url: config.url\n            \n        return (path, params, done) ->\n            options.headers = config.headers\n            options.path    = path\n            options.params  = params\n            \n            options = parseOptions options\n            return call options, done || params\n            \n    return\n        setHeader: (key, value) ->\n            config.headers[key] = value\n            return @\n            \n        get: (path, params, done) ->\n            options = parseOptions({\n                method: 'GET'\n                url: config.url\n                path: path\n                params: params\n                headers: config.headers\n            })\n            task    = call.bind call, options\n            key     = new Buffer(JSON.stringify(['GET', path])).toString('base64')\n            \n            return queue.run key, task, done || params\n        \n        post: methodFactory('POST')\n        put: methodFactory('PUT')\n        del: methodFactory('DELETE')\n        ##\n        # Initialize a streaming request for an image.\n        # example:\n        # ```\n        # router.get \"/image/:id\", (req, res, next) ->\n        #   imgReq  = api.image(req.params.id)\n        #   imgReq.on 'error', next\n        #   return imgReq.pipe(res)\n        #\n        image: (id, req, res, next) ->\n          reqUrl  = [url, 'image', id].join '/'\n          imgReq  = request reqUrl\n          \n          imReq.on 'error', next\n          return imReq.pipe(res)","undoManager":{"mark":-2017,"position":100,"stack":[[{"group":"doc","deltas":[{"action":"insertText","range":{"start":{"row":69,"column":32},"end":{"row":69,"column":33}},"text":","}]}],[{"group":"doc","deltas":[{"action":"insertText","range":{"start":{"row":69,"column":33},"end":{"row":69,"column":34}},"text":" "}]}],[{"group":"doc","deltas":[{"action":"insertText","range":{"start":{"row":69,"column":34},"end":{"row":69,"column":35}},"text":"t"}]}],[{"group":"doc","deltas":[{"action":"insertText","range":{"start":{"row":69,"column":35},"end":{"row":69,"column":36}},"text":"a"}]}],[{"group":"doc","deltas":[{"action":"insertText","range":{"start":{"row":69,"column":36},"end":{"row":69,"column":37}},"text":"s"}]}],[{"group":"doc","deltas":[{"action":"insertText","range":{"start":{"row":69,"column":37},"end":{"row":69,"column":38}},"text":"k"}]}],[{"group":"doc","deltas":[{"action":"insertText","range":{"start":{"row":69,"column":38},"end":{"row":69,"column":39}},"text":","}]}],[{"group":"doc","deltas":[{"action":"insertText",